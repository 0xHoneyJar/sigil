{
  "name": "@thehoneyjar/sigil-lens",
  "version": "0.1.0",
  "description": "Address impersonation for testing different user states",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wagmi": {
      "import": "./dist/wagmi.js",
      "types": "./dist/wagmi.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "wagmi": {
      "optional": true
    }
  },
  "dependencies": {
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.45",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "viem": "^2.0.0",
    "vitest": "^1.2.0",
    "wagmi": "^2.0.0"
  },
  "keywords": [
    "sigil",
    "lens",
    "impersonation",
    "web3",
    "testing"
  ],
  "author": "Sigil Framework",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xHoneyJar/sigil.git",
    "directory": "packages/lens"
  },
  "bugs": {
    "url": "https://github.com/0xHoneyJar/sigil/issues"
  },
  "homepage": "https://github.com/0xHoneyJar/sigil/tree/main/packages/lens#readme",
  "engines": {
    "node": ">=20.0.0"
  }
}
