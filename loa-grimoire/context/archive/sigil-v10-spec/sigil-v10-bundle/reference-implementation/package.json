{
  "name": "@sigil/soul-engine",
  "version": "10.0.0",
  "description": "Design context framework for AI-assisted product development",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./material": {
      "import": "./dist/material/index.mjs",
      "require": "./dist/material/index.js"
    },
    "./sync": {
      "import": "./dist/sync/index.mjs",
      "require": "./dist/sync/index.js"
    },
    "./hooks": {
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    },
    "./workbench": {
      "import": "./dist/workbench/index.mjs",
      "require": "./dist/workbench/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "design-system",
    "ai",
    "claude",
    "material",
    "physics",
    "sync",
    "crdt",
    "local-first"
  ],
  "author": "Sigil",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/sigil"
  }
}
